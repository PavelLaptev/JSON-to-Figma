{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "await-promise": false,
        "interface-name": false,
        "quotemark": false,
        "curly": false,
        "member-access": [
            true,
            "no-public"
        ],
        "semicolon": false,
        "no-trailing-whitespace": false,
        "no-console": false,
        "jsx-wrap-multiline": false,
        "jsx-no-lambda": false
    },
    "rulesDirectory": []
}